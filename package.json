{
	"name": "uzabo-server",
	"version": "1.0.0",
	"engines": {
		"node": "12.16.0",
		"npm": "6.13.4"
	},
	"scripts": {
		"client": "cd client && yarn start",
		"server": "nodemon server.js",
		"build": "cd client && npm run build",
		"dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
		"start": "node server.js",
		"heroku-postbuild": "npm rebuild node-sass && cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
	},
	"dependencies": {
		"@date-io/date-fns": "^2.10.6",
		"@material-ui/core": "^4.11.0",
		"@material-ui/icons": "^4.9.1",
		"@material-ui/lab": "^4.0.0-alpha.56",
		"@material-ui/pickers": "^3.2.10",
		"@material-ui/styles": "^4.10.0",
		"body-parser": "^1.18.3",
		"cors": "2.8.5",
		"dotenv": "^8.2.0",
		"express": "^4.16.4",
		"node-sass": "^4.14.1",
		"nodemailer": "^6.4.11",
		"react-carousel-with-thumbnail": "^1.4.0",
		"react-google-maps": "^9.4.5",
		"react-paypal-button-v2": "^2.6.2",
		"react-scripts": "^3.4.3",
		"react-share": "^4.3.1",
		"recompose": "^0.30.0",
		"stripe": "6.28.0",
		"styled-components": "^5.2.0",
		"web-social-share": "^6.4.1"
	},
	"devDependencies": {
		"concurrently": "^4.0.1",
		"nodemon": "^1.19.1"
	}
}
